Type: Package
Package: gge
Title: Genotype Plus Genotype-by-Environment Biplots
Version: 1.9
Authors@R: c(
    person("Kevin", "Wright", , "kw.stat@gmail.com", role = c("aut", "cre", "cph"),
           comment = c(ORCID = "0000-0002-0617-8673")),
